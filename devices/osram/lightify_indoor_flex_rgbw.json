          "refresh.interval": 300,
          "read": {
            "at": "0x0008",
            "cl": "0x0300",
            "ep": 0,
            "fn": "zcl:attr"
          },
          "parse": {
            "at": "0x0008",
            "cl": "0x0300",
            "ep": 0,
            "eval": "if (Attr.val <= 2) Item.val = ['hs', 'xy', 'ct'][Attr.val]",
            "fn": "zcl:attr"
          }
        },
        {
          "name": "state/ct",
          "description": "The current Mired color temperature of the light. Where Mired is 1000000 / color temperature (in kelvins).",
          "refresh.interval": 5,
          "read": {
            "at": "0x0007",
            "cl": "0x0300",
            "ep": 0,
            "fn": "zcl:attr"
          },
          "parse": {
            "at": "0x0007",
            "cl": "0x0300",
            "ep": 0,
            "eval": "Item.val = Attr.val",
            "fn": "zcl:attr"
