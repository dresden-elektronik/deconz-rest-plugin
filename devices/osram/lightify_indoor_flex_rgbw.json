          "refresh.interval": 300,
          "read": {
            "at": "0x4002",
            "cl": "0x0300",
            "ep": 0,
            "fn": "zcl:attr"
          },
          "parse": {
            "at": "0x4002",
            "cl": "0x0300",
            "ep": 0,
            "eval": "Item.val = Attr.val ? 'colorloop' : 'none'",
            "fn": "zcl:attr"
          },
          "default": "none"
        },
        {
          "name": "state/hue",
