{
  "schema": "devcap1.schema.json",
  "manufacturername": "OSRAM",
  "modelid": "Classic A60 RGBW",
  "product": "Classic A60 RGBW",
  "sleeper": false,
  "supportsMgmtBind": false,
  "status": "Gold",
  "subdevices": [
    {
      "type": "$TYPE_EXTENDED_COLOR_LIGHT",
      "restapi": "/lights",
      "uuid": [
        "$address.ext",
        "0x03"
      ],
      "items": [
        {
          "name": "attr/id"
        },
        {
          "name": "attr/lastannounced"
        },
        {
          "name": "attr/lastseen"
        },
        {
          "name": "attr/manufacturername"
        },
        {
          "name": "attr/modelid"
        },
        {
          "name": "attr/name"
        },
        {
          "name": "attr/swversion"
        },
        {
          "name": "attr/type"
        },
        {
          "name": "attr/uniqueid"
        },
        {
          "name": "cap/bri/move_with_onoff",
          "static": true
        },
        {
          "name": "cap/color/capabilities",
          "refresh.interval": 86400
        },
        {
          "name": "cap/color/ct/max",
          "default": 500
        },
        {
          "name": "cap/color/ct/min",
          "refresh.interval": 86400,
          "default": 151
        },
        {
          "name": "cap/on/off_with_effect",
          "static": true
        },
        {
          "name": "config/bri/on_level",
          "refresh.interval": 3600,
          "default": 255
        },
        {
          "name": "cap/color/xy/blue_x",
          "static": 0
        },
        {
          "name": "cap/color/xy/blue_y",
          "static": 0
        },
        {
          "name": "cap/color/xy/green_x",
          "static": 0
        },
        {
          "name": "cap/color/xy/green_y",
          "static": 65279
        },
        {
          "name": "cap/color/xy/red_x",
          "static": 65279
        },
        {
          "name": "cap/color/xy/red_y",
          "static": 0
        },
        {
          "name": "state/alert",
          "default": "none"
        },
        {
          "name": "state/bri",
          "refresh.interval": 20
        },
        {
          "name": "state/colormode",
          "refresh.interval": 20
        },
        {
          "name": "state/ct",
          "refresh.interval": 20
        },
        {
          "name": "state/effect",
          "refresh.interval": 60,
          "default": "none"
        },
        {
          "name": "state/hue",
          "parse": {
            "fn": "zcl:attr",
            "ep": 3,
            "cl": "0x0300",
            "at": "0x0000",
            "eval": "Item.val = Attr.val * 256"
          },
          "read": {
            "fn": "zcl:attr",
            "ep": 3,
            "cl": "0x0300",
            "at": "0x0000"
          },
          "refresh.interval": 20
        },
        {
          "name": "state/on",
          "refresh.interval": 20
        },
        {
          "name": "state/reachable"
        },
        {
          "name": "state/sat",
          "refresh.interval": 20
        },
        {
          "name": "state/x",
          "refresh.interval": 20
        },
        {
          "name": "state/y"
        }
      ]
    }
  ]
}
