        },
        {
          "name": "config/on"
        },
        {
          "name": "config/reachable"
        },
        {
          "name": "state/current",
          "refresh.interval": 360
        },
        {
          "name": "state/lastupdated"
        },
        {
          "name": "state/power",
          "refresh.interval": 360,
          "parse": {
            "at": "0x050B",
            "cl": "0x0B04",
            "ep": 1,
            "eval": "Item.val = Math.round((Attr.val * 1123) / 10000);",
            "fn": "zcl:attr"
          }
        },
        {
          "name": "state/voltage",
          "refresh.interval": 360,
          "parse": {
            "at": "0x0505",
            "cl": "0x0B04",
            "ep": 1,
            "eval": "Item.val = Math.round(Attr.val / 10);",
            "fn": "zcl:attr"
          }
        }
      ]
    },
    {
      "type": "$TYPE_CONSUMPTION_SENSOR",
      "restapi": "/sensors",
