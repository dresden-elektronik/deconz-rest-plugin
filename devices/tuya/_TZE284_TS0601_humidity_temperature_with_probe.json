          "parse": {
            "fn": "tuya",
            "dpid": 3,
            "eval": "if (Attr.val == 0) { Item.val=25 } else if (Attr.val == 1) { Item.val=50 } else { Item.val=100 }"
          },
          "read": {
            "fn": "none"
          },
          "default": 0
        },
        {
          "name": "config/offset",
