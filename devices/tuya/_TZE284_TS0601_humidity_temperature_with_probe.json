          "name": "config/on"
        },
        {
          "name": "config/reachable"
        },
        {
          "name": "state/humidity",
          "parse": {
            "fn": "tuya",
            "dpid": 2,
            "eval": "Item.val = (Attr.val * 100) + R.item('config/offset').val;"
          },
          "read": {
            "fn": "none"
          },
          "default": 0
