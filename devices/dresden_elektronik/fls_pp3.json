{
  "schema": "devcap1.schema.json",
  "manufacturername": ["dresden elektronik", "dresden elektronik"],
  "modelid": ["FLS-PP3", "FLS-PP3 White"],
  "product": "FLS-PP lp",
  "sleeper": false,
  "status": "Gold",
  "subdevices": [
    {
      "type": "$TYPE_EXTENDED_COLOR_LIGHT",
      "restapi": "/lights",
      "uuid": [
        "$address.ext",
        "0x0a"
      ],
      "items": [
        {
          "name": "attr/id"
        },
        {
          "name": "attr/lastannounced"
        },
        {
          "name": "attr/lastseen"
        },
        {
          "name": "attr/manufacturername"
        },
        {
          "name": "attr/modelid",
          "static": "FLS-PP3",
          "parse": {"fn": "zcl", "ep": 10, "cl": "0x0000", "at": "0x0005", "eval": "Item.val = Attr.val"}
        },
        {
          "name": "attr/name"
        },
        {
          "name": "attr/swversion"
        },
        {
          "name": "attr/type"
        },
        {
          "name": "attr/uniqueid"
        },
        {
          "name": "config/colorcapabilities",
          "refresh.interval": 84000
        },
        {
          "name": "config/ctmax",
          "refresh.interval": 84000
        },
        {
          "name": "config/ctmin",
          "refresh.interval": 84000
        },
        {
          "name": "config/powerup"
        },
        {
          "name": "state/alert",
          "default": "none"
        },
        {
          "name": "state/bri",
          "refresh.interval": 5
        },
        {
          "name": "state/colormode",
          "refresh.interval": 300,
          "read": {
            "at": "0x0008",
            "cl": "0x0300",
            "ep": 10,
            "fn": "zcl"
          },
          "parse": {
            "at": "0x0008",
            "cl": "0x0300",
            "ep": 10,
            "fn": "zcl",
            "script": "../generic/color_control_cluster/parse_color_mode.js"
          }
        },
        {
          "name": "state/ct",
          "refresh.interval": 5
        },
        {
          "name": "state/effect",
          "default": "none"
        },
        {
          "name": "state/hue"
        },
        {
          "name": "state/on",
          "refresh.interval": 5
        },
        {
          "name": "state/reachable"
        },
        {
          "name": "state/sat",
          "refresh.interval": 5
        },
        {
          "name": "state/x",
          "refresh.interval": 5
        },
        {
          "name": "state/y"
        }
      ]
    },
    {
      "type": "$TYPE_DIMMABLE_LIGHT",
      "restapi": "/lights",
      "uuid": [
        "$address.ext",
        "0x0b"
      ],
      "items": [
        {
          "name": "attr/id"
        },
        {
          "name": "attr/lastannounced"
        },
        {
          "name": "attr/lastseen"
        },
        {
          "name": "attr/manufacturername"
        },
        {
          "name": "attr/modelid",
          "static": "FLS-PP3 White",
          "parse": {"fn": "zcl", "ep": 11, "cl": "0x0000", "at": "0x0005", "eval": "Item.val = Attr.val"}
        },
        {
          "name": "attr/name"
        },
        {
          "name": "attr/swversion"
        },
        {
          "name": "attr/type"
        },
        {
          "name": "attr/uniqueid"
        },
        {
          "name": "config/powerup"
        },
        {
          "name": "state/alert",
          "default": "none"
        },
        {
          "name": "state/bri",
          "refresh.interval": 5
        },
        {
          "name": "state/on",
          "refresh.interval": 5
        }
      ]
    }
  ]
}
