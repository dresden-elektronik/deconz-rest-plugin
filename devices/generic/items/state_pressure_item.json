{
	"schema": "resourceitem1.schema.json",
	"id": "state/pressure",
	"datatype": "Int16",
	"access": "R",
	"public": true,
	"description": "The current air pressure (mBar).",
	"parse": {
		"at": "0x0000",
		"cl": "0x0403",
		"ep": 0,
		"eval": "Item.val = Attr.val + R.item('config/offset').val",
		"fn": "zcl"
	},
	"default": 0
}