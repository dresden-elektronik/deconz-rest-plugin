{
  "schema": "resourceitem1.schema.json",
  "id": "state/flow",
  "datatype": "Int16",
  "access": "R",
  "public": true,
  "description": "The current relative flow in m3/h.",
  "parse": {
    "at": "0x0000",
    "cl": "0x0404",
    "ep": 0,
    "eval": "Item.val = Attr.val / 10 + R.item('config/offset').val",
    "fn": "zcl:attr"
  },
  "default": 0
}
