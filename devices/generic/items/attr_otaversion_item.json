{
  "schema": "resourceitem1.schema.json",
  "id": "attr/otaversion",
  "datatype": "UInt32",
  "access": "R",
  "public": true,
  "implicit": false,
  "description": "OTA cluster file version.",
  "parse": {
    "fn": "zcl:attr",
    "ep": 255,
    "cl": "0x0019",
    "at": "0x0002",
    "eval": "Item.val = Attr.val"
  },
  "read": {
    "fn": "zcl:attr",
    "ep": 0,
    "cl": "0x0019",
    "at": "0x0002"
  },
  "refresh.interval": 86400
}
