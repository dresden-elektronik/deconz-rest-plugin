{
	"schema": "resourceitem1.schema.json",
	"id": "state/ct",
	"datatype": "UInt16",
	"access": "RW",
	"public": true,
	"description": "The current Mired color temperature of the light. Where Mired is 1000000 / color temperature (in kelvins).",
	"parse": {"fn": "zcl", "ep": 0, "cl": "0x0300", "at": "0x0007", "eval": "Item.val = Attr.val"},
	"read": {"fn": "zcl", "ep": 0, "cl": "0x0300", "at": "0x0007"},
	"refresh.interval": 5
}