{
  "schema": "resourceitem1.schema.json",
  "id": "state/voltage",
  "datatype": "UInt16",
  "access": "RW",
  "public": true,
  "description": "The measured voltage (in V).",
  "read": {
    "at": "0x0505",
    "cl": "0x0b04",
    "ep": 0,
    "fn": "zcl:attr"
  },
  "parse": {
    "at": "0x0505",
    "cl": "0x0b04",
    "ep": 0,
    "eval": "if (Attr.val != 65535) { Item.val = Attr.val; }"
  },
  "refresh.interval": 300,
  "default": 0
}
