{
    "schema": "resourceitem1.schema.json",
    "id": "cap/color/gradient/pixel_count",
    "datatype": "UInt8",
    "access": "R",
    "public": true,
    "description": "Number of pixels on Hue gradient light.",
    "parse": {
        "fn": "zcl", "ep": "0x0b", "cl": "0xfc03", "mf": "0x100b", "at": "0x0030",
        "eval": "Item.val = Attr.val"
    },
    "read": {
        "fn": "zcl", "ep": "0x0b", "cl": "0xfc03", "mf": "0x100b", "at": ["0x0030", "0x0031", "0x0036"]
    },
    "refresh.interval": 86400
}
