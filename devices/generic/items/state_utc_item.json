{
	"schema": "resourceitem1.schema.json",
	"id": "state/utc",
	"datatype": "ISO 8601 timestamp",
	"access": "RW",
	"public": true,
	"description": "Current timestamp in UTC set on the device.",
	"parse": {"fn": "time"},
	"read": {"at": "0x0000", "cl": "0x000A", "ep": 0, "fn": "zcl"},
	"refresh.interval": 3600
}