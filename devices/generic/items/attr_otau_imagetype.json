{
  "schema": "resourceitem1.schema.json",
  "id": "attr/otau/imagetype",
  "datatype": "UInt16",
  "access": "R",
  "public": true,
  "implicit": false,
  "description": "OTAU image type.",
  "parse": {
    "fn": "zcl:cmd",
    "ep": 255,
    "cl": "0x0019",
    "cmd": "0x01",
    "eval": "Item.val = ZclFrame.at(3) << 8 | ZclFrame.at(3)"
  },
  "read": {
    "fn": "zcl:cmd",
    "ep": 0,
    "cl": "0x0019",
    "cmd": "0x00"
  },
  "refresh.interval": 86400
}
