{
  "schema": "resourceitem1.schema.json",
  "id": "state/current_P1",
  "datatype": "UInt16",
  "access": "RW",
  "public": true,
  "default": 0,
  "description": "The measured current on phase 1.",
  "read": {
    "at": "0x0508",
    "cl": "0x0b04",
