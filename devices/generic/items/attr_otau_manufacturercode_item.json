{
  "schema": "resourceitem1.schema.json",
  "id": "attr/otau/manufacturercode",
  "datatype": "UInt16",
  "access": "R",
  "public": true,
  "implicit": false,
  "description": "OTA manufacturer code.",
  "parse": {
    "fn": "zcl:cmd",
    "ep": 255,
    "cl": "0x0019",
    "cmd": "0x01",
    "eval": "Item.val = ZclFrame.at(1) << 8 | ZclFrame.at(0)"
  }
}
