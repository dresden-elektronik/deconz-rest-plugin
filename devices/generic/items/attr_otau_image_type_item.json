{
  "schema": "resourceitem1.schema.json",
  "id": "attr/otau/image_type",
  "datatype": "UInt16",
  "access": "R",
  "public": true,
  "implicit": false,
  "description": "Firmware image type.",
  "parse": {
    "fn": "zcl:cmd",
    "ep": 255,
    "cl": "0x0019",
    "cmd": "0x01",
    "eval": "if (ZclFrame.at(0) === 0) Item.val = ZclFrame.at(4) << 8 | ZclFrame.at(3)"
  }
}
