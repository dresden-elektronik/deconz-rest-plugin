{
  "schema": "resourceitem1.schema.json",
  "id": "attr/otau/version",
  "datatype": "UInt32",
  "access": "R",
  "public": true,
  "implicit": false,
  "description": "OTAU version.",
  "parse": {
    "fn": "zcl:cmd",
    "ep": 255,
    "cl": "0x0019",
    "cmd": "0x01",
    "eval": "Item.val = ZclFrame.at(7) << 24 | ZclFrame.at(6) << 16 | ZclFrame.at(5) << 8 | ZlcFrame.at(4)"
  }
}
