{
  "schema": "devcap1.schema.json",
  "manufacturername": [
    "$MF_SIGNIFY",
    "$MF_PHILIPS"
  ],
  "modelid": [
    "SOC001",
    "SOC001"
  ],
  "product": "Hue secure contact sensor",
  "status": "Gold",
  "sleeper": false,
  "subdevices": [
    {
      "type": "$TYPE_OPEN_CLOSE_SENSOR",
      "restapi": "/sensors",
      "uuid": [
        "$address.ext",
        "0x02",
        "0xfc06"
      ],
      "fingerprint": {
        "endpoint": "0x02",
        "profile": "0x0104",
        "device": "0x0850",
        "in": [
          "0x0000",
          "0x0001",
          "0x0003",
          "0xfc06"
        ],
        "out": [
          "0x0006",
          "0x0019"
        ]
      },
      "items": [
        {
          "name": "attr/lastseen"
        },
        {
          "name": "attr/manufacturername"
        },
        {
          "name": "attr/modelid"
        },
        {
          "name": "attr/name"
        },
        {
          "name": "attr/productname",
          "static": "Hue secure contact sensor"
        },
        {
          "name": "attr/swversion"
        },
        {
          "name": "attr/type"
        },
        {
          "name": "attr/uniqueid"
        },
        {
          "name": "config/battery",
          "parse": {
            "fn": "zcl:attr",
            "ep": 2,
            "cl": "0x0001",
            "at": "0x0021",
            "eval": "Item.val = Math.round(Attr.val / 2)"
          },
          "awake": true
        },
        {
          "name": "config/group",
          "default": "auto"
        },
        {
          "name": "config/on"
        },
        {
          "name": "config/reachable"
        },
        {
          "name": "state/eventduration",
          "parse": {
            "fn": "zcl:attr",
            "ep": 2,
            "cl": "0xfc06",
            "mf": "0x100b",
            "at": "0x0101",
            "eval": "Item.val = Attr.val"
          },
          "awake": true,
          "read": {
            "fn": "none"
          }
        },
        {
          "name": "state/open",
          "parse": {
            "fn": "zcl:attr",
            "ep": 2,
            "cl": "0xfc06",
            "mf": "0x100b",
            "at": "0x0100",
            "eval": "Item.val = Attr.val === 1"
          },
          "awake": true,
          "read": {
            "fn": "zcl:attr",
            "ep": 2,
            "cl": "0xfc06",
            "mf": "0x100b",
            "at": [
              "0x0100",
              "0x0101",
              "0x0102",
              "0x0103"
            ]
          },
          "refresh.interval": 360
        },
        {
          "name": "state/lastupdated"
        },
        {
          "name": "state/tampered",
          "parse": {
            "fn": "zcl:attr",
            "ep": 2,
            "cl": "0xfc06",
            "mf": "0x100b",
            "at": "0x0102",
            "eval": "Item.val = Attr.val === 1"
          },
          "awake": true,
          "read": {
            "fn": "none"
          }
        }
      ]
    }
  ],
  "bindings": [
    {
      "bind": "unicast",
      "src.ep": 2,
      "cl": "0xfc06",
      "report": [
        {
          "mf": "0x100b",
          "at": "0x0100",
          "dt": "0x30",
          "min": 1,
          "max": 300
        },
        {
          "mf": "0x100b",
          "at": "0x0101",
          "dt": "0x23",
          "min": 1,
          "max": 300,
          "change": "0x00000001"
        },
        {
          "mf": "0x100b",
          "at": "0x0102",
          "dt": "0x30",
          "min": 1,
          "max": 300
        },
        {
          "mf": "0x100b",
          "at": "0x0103",
          "dt": "0x23",
          "min": 1,
          "max": 300,
          "change": "0x00000001"
        }
      ]
    },
    {
      "bind": "unicast",
      "src.ep": 2,
      "cl": "0x0001",
      "report": [
        {
          "at": "0x0021",
          "dt": "0x20",
          "min": 300,
          "max": 300,
          "change": "0x01"
        }
      ]
    },
    {
      "bind": "groupcast",
      "src.ep": 2,
      "cl": "0x0006",
      "config.group": 0
    }
  ]
}
