                        ],
                        [
                            "\"effect\"",
                            "dynamic effect"
                        ],
                        [
                            "\"gradient\"",
                            "gradient"
                        ],
                        [
                            "\"hs\"",
                            "hue and saturation"
                        ],
                        [
                            "\"xy\"",
                            "CIE xy color space coordinates"
                        ]
                    ],
                    "parse": {
                        "fn": "zcl",
                        "ep": "0x0b",
                        "cl": "0x0300",
                        "at": "0x4001",
                        "eval": "if (Item.val !== 'effect' && Item.val !== 'gradient' && Attr.val <= 3) Item.val = ['hs', 'xy', 'ct', 'xy'][Attr.val]"
                    },
                    "read": {
                        "fn": "zcl",
                        "ep": "0x0b",
                        "cl": "0x0300",
                        "at": [
                            "0x4001",
                            "0x4002",
                            "0x4000",
                            "0x0001"
                        ]
                    },
                    "refresh.interval": 60
                },
                {
                    "name": "state/effect",
                    "values": [
                        [
                            "\"none\"",
                            "no effect is active"
                        ],
                        [
                            "\"colorloop\"",
                            "colorloop through hue values"
                        ],
                        [
                            "\"candle\"",
                            "candlelight dynamic effect"
                        ],
                        [
                            "\"fireplace\"",
                            "fireplace dynamic effect"
                        ],
                        [
                            "\"loop\"",
                            "colorloop dynamic effect"
                        ],
                        [
                            "\"sunrise\"",
                            "sunrise dynamic effect"
                        ]
                    ],
                    "parse": {
                        "fn": "zcl",
                        "ep": "0x0b",
                        "cl": "0x0300",
                        "at": "0x4002",
                        "eval": "if (Item.val === 'none' || Item.val === 'colorloop') Item.val = Attr.val ? 'colorloop' : 'none'"
                    },
                    "read": {
                        "fn": "none"
                    }
                },
                {
                    "name": "state/hue",
                    "read": {
