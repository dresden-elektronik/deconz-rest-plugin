{
  "schema": "devcap1.schema.json",
  "manufacturername": [
    "$MF_PHILIPS",
    "$MF_PHILIPS",
    "$MF_PHILIPS",
    "$MF_PHILIPS",
    "$MF_PHILIPS",
    "$MF_SIGNIFY",
    "$MF_SIGNIFY",
    "$MF_SIGNIFY",
    "$MF_SIGNIFY",
    "$MF_SIGNIFY"
  ],
  "modelid": [
    "LTA001",
    "LTE002",
    "LTG002",
    "LTV002",
    "915005997701",
    "LTA001",
    "LTE002",
    "LTG002",
    "LTV002",
    "915005997701"
  ],
  "product": "Hue white ambiance light",
  "sleeper": false,
  "status": "Gold",
  "subdevices": [
    {
      "type": "$TYPE_COLOR_TEMPERATURE_LIGHT",
      "restapi": "/lights",
      "uuid": [
        "$address.ext",
        "0x0b"
      ],
      "items": [
        {
          "name": "attr/id"
        },
        {
          "name": "attr/lastannounced"
        },
        {
          "name": "attr/lastseen"
        },
        {
          "name": "attr/manufacturername",
          "parse": {
            "fn": "zcl:attr",
            "ep": "0x0b",
            "cl": "0x0000",
            "at": "0x0004",
            "eval": "Item.val = Attr.val"
          },
          "read": {
            "fn": "zcl:attr",
            "ep": "0x0b",
            "cl": "0x0000",
            "at": "0x0004"
          },
          "refresh.interval": 86400
        },
        {
          "name": "attr/modelid",
          "parse": {
            "fn": "zcl:attr",
            "ep": "0x0b",
            "cl": "0x0000",
            "at": "0x0005",
            "eval": "Item.val = Attr.val"
          },
          "read": {
            "fn": "zcl:attr",
            "ep": "0x0b",
            "cl": "0x0000",
            "at": "0x0005"
          },
          "refresh.interval": 86400
        },
        {
          "name": "attr/name"
        },
        {
          "name": "attr/productid"
        },
        {
          "name": "attr/productname",
          "parse": {
            "fn": "zcl:cmd",
            "ep": "0x0b",
            "cl": "0x0000",
            "mf": "0x100b",
            "cmd": "0xc1",
            "script": "0000_capabilities.js"
          },
          "read": {
            "fn": "zcl:cmd",
            "ep": "0x0b",
            "cl": "0x0000",
            "mf": "0x100b",
            "cmd": "0xc0",
            "eval": "'00' + (15 + R.item('attr/modelid').val.length + R.item('attr/manufacturername').val.length).toString(16) + '00000040'"
          },
          "refresh.interval": 86400
        },
        {
          "name": "attr/swconfigid"
        },
        {
          "name": "attr/swversion"
        },
        {
          "name": "attr/type"
        },
        {
          "name": "attr/uniqueid"
        },
        {
          "name": "cap/alert/trigger_effect"
        },
        {
          "name": "cap/bri/min_dim_level"
        },
        {
          "name": "cap/bri/move_with_onoff"
        },
        {
          "name": "cap/color/capabilities"
        },
        {
          "name": "cap/color/ct/max"
        },
        {
          "name": "cap/color/ct/min"
        },
        {
          "name": "cap/on/off_with_effect"
        },
        {
          "name": "config/bri/execute_if_off"
        },
        {
          "name": "config/bri/startup"
        },
        {
          "name": "config/color/ct/startup"
        },
        {
          "name": "config/color/execute_if_off"
        },
        {
          "name": "config/on/startup"
        },
        {
          "name": "state/alert"
        },
        {
          "name": "state/on",
          "parse": {
            "fn": "zcl:attr",
            "ep": "0x0b",
            "cl": "0xfc03",
            "mf": "0x100b",
            "at": "0x0002",
            "script": "fc03_state.js"
          },
          "read": {
            "fn": "zcl:attr",
            "ep": "0x0b",
            "cl": "0xfc03",
            "mf": "0x100b",
            "at": "0x0002"
          },
          "refresh.interval": 360
        },
        {
          "name": "state/bri",
          "read": {
            "fn": "none"
          }
        },
        {
          "name": "state/ct",
          "read": {
            "fn": "none"
          }
        },
        {
          "name": "state/colormode",
          "values": [
            [
              "\"ct\"",
              "color temperature"
            ],
            [
              "\"effect\"",
              "dynamic effect"
            ]
          ],
          "parse": {
            "fn": "zcl:attr",
            "ep": "0x0b",
            "cl": "0x0300",
            "at": "0x4001",
            "eval": "if (Item.val !== 'effect' && Attr.val <= 3) Item.val = ['hs', 'xy', 'ct', 'xy'][Attr.val]"
          },
          "read": {
            "fn": "zcl:attr",
            "ep": "0x0b",
            "cl": "0x0300",
            "at": [
              "0x4001",
              "0x4002",
              "0x4000",
              "0x0001"
            ]
          },
          "refresh.interval": 60
        },
        {
          "name": "state/effect",
          "values": [
            [
              "\"none\"",
              "no effect is active"
            ],
            [
              "\"candle\"",
              "candlelight dynamic effect"
            ]
          ],
          "read": {
            "fn": "none"
          }
        },
        {
          "name": "state/reachable"
        }
      ]
    }
  ],
  "bindings": [
    {
      "bind": "unicast",
      "src.ep": 11,
      "cl": "0xfc03",
      "report": [
        {
          "mf": "0x100b",
          "at": "0x0002",
          "dt": "0x41",
          "min": 1,
          "max": 300
        }
      ]
    }
  ]
}
