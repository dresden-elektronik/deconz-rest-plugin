          "name": "config/on/startup"
        },
        {
          "name": "state/alert"
        },
        {
          "name": "state/on",
          "parse": {
            "fn": "zcl:attr",
            "ep": "0x0b",
            "cl": "0xfc03",
            "mf": "0x100b",
            "at": "0x0002",
            "script": "fc03_state.js"
          },
          "read": {
            "fn": "zcl:attr",
            "ep": "0x0b",
            "cl": "0xfc03",
            "mf": "0x100b",
            "at": "0x0002"
          },
          "refresh.interval": 5
        },
        {
          "name": "state/bri",
          "read": {
            "fn": "none"
          }
        },
        {
          "name": "state/ct",
          "read": {
            "fn": "none"
          }
        },
        {
          "name": "state/x",
          "read": {
            "fn": "none"
          }
        },
        {
          "name": "state/y",
          "read": {
            "fn": "none"
          }
        },
        {
          "name": "state/colormode",
          "parse": {
            "fn": "zcl:attr",
            "ep": "0x0b",
            "cl": "0x0300",
            "at": "0x4001",
            "eval": "if (Item.val !== 'effect' && Attr.val <= 3) Item.val = ['hs', 'xy', 'ct', 'xy'][Attr.val]"
          },
          "read": {
            "fn": "zcl:attr",
            "ep": "0x0b",
            "cl": "0x0300",
            "at": [
              "0x4001",
              "0x4002",
              "0x4000",
              "0x0001"
            ]
          },
          "refresh.interval": 60
        },
        {
          "name": "state/effect",
          "values": [
            [
              "\"none\"",
              "no effect is active"
            ],
            [
              "\"colorloop\"",
              "colorloop through hue values"
            ],
            [
              "\"candle\"",
              "candlelight dynamic effect"
            ],
            [
              "\"fireplace\"",
              "fireplace dynamic effect"
            ],
            [
              "\"prism\"",
              "prism dynamic effect"
            ]
          ],
          "parse": {
            "fn": "zcl:attr",
            "ep": "0x0b",
            "cl": "0x0300",
            "at": "0x4002",
            "eval": "if (Item.val === 'none' || Item.val === 'colorloop') Item.val = Attr.val ? 'colorloop' : 'none'"
          },
          "read": {
            "fn": "none"
          }
        },
        {
          "name": "state/hue",
          "read": {
            "fn": "none"
          }
        },
        {
          "name": "state/sat",
          "read": {
            "fn": "none"
          }
        },
        {
          "name": "state/reachable"
        }
