          "parse": {
            "fn": "zcl:attr",
            "ep": "0x0b",
            "cl": "0x0000",
            "at": "0x0004",
            "eval": "Item.val = Attr.val"
          },
          "read": {
            "fn": "zcl:attr",
            "ep": "0x0b",
            "cl": "0x0000",
            "at": "0x0004"
          },
